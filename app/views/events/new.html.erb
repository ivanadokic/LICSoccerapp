<h1>New Event</h1>
<%= form_for @event do |e|%>
  <%= e.label :location%><br>
  <%= e.text_field :location%><br>

  <%= e.label :event_type%><br>
  <%= e.text_field :event_type%><br>

  <%= e.label :start%><br>
  <%= e.datetime_field :start%><br>

  <%= e.label :end%><br>
  <%= e.datetime_field :end%><br>

  <%= submit_tag "Create"%>
  
<%end%>

<%= button_to "Logout", '/logout', method: :delete%>